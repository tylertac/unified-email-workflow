---
layout: layout.hbs
subject: Grid Template
---

<table {{> table-default }}>
  <tbody>
    {{> head-bg-grey-engineering }}
    {{> header-dos-engineering }}
    <tr>
      <td {{> td-default-outside }}>
        <table {{> table-default }}>
          <tr>
            <td {{> td-default }}>
              {{> notice type='warning' text='<strong>Maintence Window Start</strong>' }}
            </td>
          </tr>
        </table>
      </td>
    </tr>
    <tr>
      <td {{> td-default-outside }} style="padding:0px 10px 0px 10px">
        <table {{> table-default }}>
          <tbody>
            {{> body-copy }}
          </tbody>
        </table>
      </td>
    </tr>
    <tr>
      <td {{> td-default-outside }} style="padding:0px 10px 0px 10px">
        <table {{> table-default }} style="display: block; height: 100%;overflow-y: scroll;" >
          <tr>
            <td >
              {{> table-release-notes-sample }}
            </td>
          </tr>
        </table>
      </td>
    </tr>
    <tr>
      <td {{> td-default-outside }} style="padding:0px 10px 0px 10px">
        <table {{> table-default }}>
          <tbody>
            {{> signoff }}
          </tbody>
        </table>
      </td>
    </tr>
    {{> footer }}
  </tbody>
</table>
